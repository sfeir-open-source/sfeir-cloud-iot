{
    "swagger": "2.0",
    "info": {
        "title": "bicycle API",
        "contact": {},
        "version": "0.1.0"
    },
    "basePath": "/v1",
    "paths": {
        "/bicycle": {
            "get": {
                "description": "Gets all the bicycle data from database",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "BicyleData"
                ],
                "summary": "Gets all bicycle data from database",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.BicycleData"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/bicycle/config": {
            "get": {
                "description": "Gets the current config from database",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "ConfigData"
                ],
                "summary": "Gets the current config from database",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.ConfigData"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/bicycle/speed": {
            "get": {
                "description": "Computes the current bicycle speed from the latest data in firebase",
                "tags": [
                    "BicyleSpeed"
                ],
                "summary": "Computes the current bicycle speed from database",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "int"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.BicycleData": {
            "type": "object",
            "properties": {
                "revolutions": {
                    "type": "integer",
                    "example": 10
                },
                "rpm": {
                    "type": "integer",
                    "example": 200
                },
                "time": {
                    "type": "string",
                    "example": "2021-10-26T22:17:36Z"
                }
            }
        },
        "models.ConfigData": {
            "type": "object",
            "properties": {
                "radius_in_cm": {
                    "type": "number",
                    "example": 30
                },
                "target_km": {
                    "type": "integer",
                    "example": 150
                }
            }
        },
        "models.ErrorResponse": {
            "type": "object",
            "properties": {
                "error_code": {
                    "type": "string",
                    "example": "UnprocessableEntity"
                },
                "message": {
                    "type": "string",
                    "example": "Malformed body"
                },
                "status_code": {
                    "type": "integer",
                    "example": 422
                }
            }
        }
    }
}